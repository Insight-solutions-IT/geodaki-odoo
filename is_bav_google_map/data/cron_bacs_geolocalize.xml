<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- A cron to geolocate existing Fleet which doesn't geolocated yet -->
    <record id="ir_cron_data_bacs_geolocate" model="ir.cron">
        <field name="name">Bacs : geolocate</field>
        <field name="model_id" ref="model_is_bav_bacs"/>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="code">model.with_context(from_cron=True).action_cron_geolocalize()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="False"/>
    </record>
</odoo>