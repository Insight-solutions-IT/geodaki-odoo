<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
  

    <record id="module_state_category" model="ir.module.category">
            <field name="name">Les Etats</field>
            <field name="sequence">1</field>
            <field name="parent_id" ref="is_temp_reel.module_Geodaki_category"/>
        </record>
        <record id="et_user" model="res.groups">
            <field name="name">Utilisateur</field>
            <!-- <field name="implied_ids" eval="[(4, ref('fleet_group_user'))]"/> -->
            <field name="category_id" ref="module_state_category"/>
        </record>
       
        <record id="et_super_admin" model="res.groups">
            <field name="name">Super Administrateur</field>
            <field name="category_id" ref="module_state_category"/>
            <field name="implied_ids" eval="[(4, ref('et_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
</odoo>
