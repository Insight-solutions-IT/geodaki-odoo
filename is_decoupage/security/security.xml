        </record>
        <record id="d_admin" model="res.groups">
            <field name="name">Administrateur</field>
            <field name="implied_ids" eval="[(4, ref('p_user'))]"/>
            <field name="category_id" ref="module_planning_category"/>
        </record>
        <record id="d_super_admin" model="res.groups">
            <field name="name">Super Administrateur</field>
            <field name="category_id" ref="module_planning_category"/>
            <field name="implied_ids" eval="[(4, ref('p_admin'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>


        

