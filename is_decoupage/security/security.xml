        <record id="module_planning_category" model="ir.module.category">
            <field name="name">Gestion des EPC</field>
            <field name="sequence">1</field>
            <field name="parent_id" ref="is_temp_reel.module_Geodaki_category"/>
        </record>
        <record id="d_user" model="res.groups">
            <field name="name">Utilisateur</field>
            <!-- <field name="implied_ids" eval="[(4, ref('fleet_group_user'))]"/> -->
