<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="is_historique.list">
      <field name="name">is_historique list</field>
      <field name="model">is_historique.calcule</field>
      <field name="arch" type="xml">
        <tree>
          <field name="datej"/>
          <field name="deviceid"/>
          <field name="acc"/>
          <field name="start_datetime"/>
          <field name="old_adresse"/>
          <field name="end_datetime"/>
          <field name="new_adresse"/>
          <field name="duree"/>
          <field name="arret"/>
          <field name="sommeD"/>
          
        </tree>
      </field>
    </record>
    <record model="ir.ui.view" id="is_historique.search">
      <field name="name">is_historique search</field>
      <field name="model">is_historique.calcule</field>
      <field name="arch" type="xml">
          <search>
              <field name="deviceid" />
              <field name="start_datetime"/>
              <field name="old_adresse"/>
              <field name="end_datetime"/>
              <field name="new_adresse"/>
          </search>
      </field>
  </record>
  


