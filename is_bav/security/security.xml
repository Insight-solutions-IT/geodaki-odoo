<?xml version="1.0" ?>
<odoo>
<data noupdate="0">
		<record id="module_Geodaki_category" model="ir.module.category">
            <field name="name">Geodaki</field>
            <field name="sequence">2</field>
        </record>
        <record id="module_temp_reel_category" model="ir.module.category">
            <field name="name">Gestion des EPC</field>
            <field name="sequence">1</field>
            <field name="parent_id" ref="module_Geodaki_category"/>
        </record>
       <record id="tr_user" model="res.groups">
            <field name="name">Utilisateur</field>
            <field name="category_id" ref="module_temp_reel_category"/>
        </record>
        <record id="tr_admin" model="res.groups">
            <field name="name">Administrateur</field>
            <field name="implied_ids" eval="[(4, ref('tr_user'))]"/>
            <field name="category_id" ref="module_temp_reel_category"/>
        </record>
        
		 <record id="tr_super_admin" model="res.groups">
            <field name="name">Super Administrateur</field>
            <field name="category_id" ref="module_temp_reel_category"/>
            <field name="implied_ids" eval="[(4, ref('tr_admin'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
		
		
		
     </data>  

</odoo>

